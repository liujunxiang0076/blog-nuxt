{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "types": ["@nuxt/types", "@types/node", "vue/runtime-core"],
    "paths": {
      "#imports": ["./.nuxt/imports"],
      "#vue": ["./node_modules/vue"],
      "~/*": ["./*"],
      "@/*": ["./*"]
    }
  }
}
